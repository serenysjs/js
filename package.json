{
	"name": "serenys",
	"version": "0.1.1",
	"description": "Serenys server side client library in JavaScript for Node.js",
	"scripts": {
		"test": "vitest run",
		"build": "tsup",
		"lint": "prettier --check . && eslint src --ext .mjs,.cjs,.js,.ts --format=pretty",
		"format": "prettier --write . && eslint src --ext .mjs,.cjs,.js,.ts --fix --format=pretty",
		"fmt": "pnpm format"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"license": "Apache-2.0",
	"keywords": [
		"api",
		"serenys",
		"discord"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/serenysjs/js.git"
	},
	"bugs": {
		"url": "https://github.com/serenysjs/js/issues"
	},
	"homepage": "https://serenys.xyz/",
	"dependencies": {
		"cross-fetch": "^3.1.6"
	},
	"devDependencies": {
		"@types/node": "^20.3.1",
		"eslint": "^8.43.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-formatter-pretty": "^5.0.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3",
		"vitest": "^0.32.2"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
