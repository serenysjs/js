{
	"name": "senerys",
	"version": "0.1.0",
	"scripts": {
		"build": "tsup",
		"lint": "prettier --check . && eslint src --ext .mjs,.cjs,.js,.ts --format=pretty",
		"format": "prettier --write . && eslint src --ext .mjs,.cjs,.js,.ts --fix --format=pretty",
		"fmt": "pnpm format"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"license": "Apache-2.0",
	"dependencies": {
		"undici": "^5.22.1"
	},
	"devDependencies": {
		"@types/node": "^20.2.5",
		"eslint": "^8.42.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-formatter-pretty": "^5.0.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3"
	},
	"engines": {
		"node": ">=16.9.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
